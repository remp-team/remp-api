{
  "name": "Library",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "title": {
      "type": "string",
      "required": true
    },
    "published": {
      "type": "boolean",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "rempUser": {
      "type": "belongsTo",
      "model": "RempUser",
      "foreignKey": ""
    },
    "playlists": {
      "type": "hasMany",
      "model": "Playlist",
      "foreignKey": ""
    }
  },
  "acls": [
    {
      "accessType": "*",
      "principalType": "ROLE",
      "principalId": "$everyone",
      "permission": "DENY"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__create__playlists"
    },
    {
      "accessType": "EXECUTE",
      "principalType": "ROLE",
      "principalId": "$owner",
      "permission": "ALLOW",
      "property": "__get__playlists"
    }
  ],
  "methods": []
}
